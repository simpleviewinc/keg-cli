{
  "name": "keg-cli",
  "displayName": "Keg CLI",
  "docker": {
    "providerUrl": "ghcr.io",
    "namespace": "simpleviewinc",
    "user": "",
    "token": ""
  },
  "cli": {
    "paths": {
      "customTasks": ""
    },
    "git": {
      "orgName": "simpleviewinc",
      "orgUrl": "https://github.com/simpleviewinc",
      "provider": "https://github.com",
      "publicToken": "",
      "repos": {
        "hub": "keg-hub",
        "cli": "keg-cli"
      }
    },
    "taps": {},
    "settings": {
      "defaultEnv": "ci",
      "docker": {
        "preConfirm": true,
        "buildKit": true,
        "force": true,
        "defaultLocalBuild": false,
        "imagePullPolicy": "Always",
        "defaultTag": "master"
      },
      "checkUpdates": false,
      "editorCmd": "code",
      "errorStack": false,
      "git": {
        "secure": false
      },
      "task": {
        "optionsAsk": false
      }
    }
  },
  "publish": {
    "default": {
      "tasks": {
        "install": true,
        "test": true,
        "build": true,
        "publish": true,
        "commit": true
      }
    }
  }
}
