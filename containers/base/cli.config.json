{
  "version": "1.2.0",
  "name": "keg-cli",
  "displayName": "Keg CLI",
  "docker": {
    "providerUrl": "ghcr.io",
    "namespace": "simpleviewinc",
    "user": "keg-public",
    "token": ""
  },
  "cli": {
    "git": {
      "orgName": "simpleviewinc",
      "orgUrl": "https://github.com/simpleviewinc",
      "provider": "https://github.com",
      "publicToken": "",
      "repos": {
        "hub": "keg-hub",
        "cli": "keg-cli"
      },
      "user": "keg-public",
      "key": "",
      "sshKey": "/root/.ssh/github"
    },
    "paths": {
      "ask": "/keg-hub/repos/ask-it",
      "arp": "/keg-hub/repos/args-parse",
      "kegConfig": "/root/.kegConfig",
      "containers": "/keg-hub/repos/keg-cli/containers",
      "cli": "/keg-hub/repos/keg-cli",
      "core": "/keg-hub/repos/keg-core",
      "external": "/keg-hub/external",
      "hub": "/keg-hub",
      "jsutils": "/keg-hub/repos/jsutils",
      "keg": "/keg-hub",
      "repos": "/keg-hub/repos",
      "resolver": "/keg-hub/repos/tap-resolver",
      "taps": "/keg-hub/taps"
    },
    "settings": {
      "defaultEnv": "development",
      "docker": {
        "preConfirm": false,
        "buildKit": true,
        "force": true,
        "defaultLocalBuild": true
      },
      "checkUpdates": false,
      "editorCmd": false,
      "errorStack": false,
      "git": {
        "secure": false
      },
      "task": {
        "ci": true,
        "optionsAsk": false
      }
    },
    "taps": {
      "retheme": {
        "path": "/keg-hub/repos/re-theme"
      },
      "components": {
        "path": "/keg-hub/repos/keg-components"
      }
    }
  },
  "publish": {
    "default": {
      "tasks": {
        "install": true,
        "test": true,
        "build": true,
        "publish": true,
        "commit": true
      }
    },
    "keg": {
      "name": "keg",
      "dependent": true,
      "order": {
        "0": "@keg-hub/re-theme",
        "1": "@keg-hub/keg-components",
        "2": "@keg-hub/keg-core"
      }
    },
    "retheme": {
      "name": "retheme",
      "dependent": false,
      "order": {
        "0": "@keg-hub/re-theme"
      }
    },
    "components": {
      "name": "components",
      "dependent": false,
      "order": {
        "0": "@keg-hub/keg-components"
      }
    },
    "core": {
      "name": "core",
      "dependent": false,
      "order": {
        "0": "@keg-hub/keg-core"
      }
    },
    "jsutils": {
      "name": "jsutils",
      "dependent": false,
      "order": {
        "0": "@keg-hub/jsutils"
      }
    },
    "resolver": {
      "name": "resolver",
      "dependent": false,
      "order": {
        "0": "@keg-hub/tap-resolver"
      }
    },
    "spawn-cmd": {
      "name": "spawn-cmd",
      "dependent": false,
      "order": {
        "0": "@keg-hub/spawn-cmd"
      }
    },
    "ask-it": {
      "name": "ask-it",
      "dependent": false,
      
      "order": {
        "0": "@keg-hub/ask-it"
      }
    },
    "args-parse": {
      "name": "args-parse",
      "dependent": false,
      "order": {
        "0": "@keg-hub/args-parse"
      }
    }
  }
}